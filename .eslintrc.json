{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-console": "off",
    "prefer-const": "off",
    "no-var": "error",
    "eqeqeq": [
      "error",
      "always"
    ],
    "curly": "off",
    "react-hooks/exhaustive-deps": "off",
    "@next/next/no-assign-module-variable": "off",
    "react/jsx-no-duplicate-props": "error",
    "react/no-unescaped-entities": "off",
    "@next/next/no-img-element": "off"
  }
}

